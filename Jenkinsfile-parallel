pipeline {
    agent any
    stages {
        stage('PARALLEL') {
            parellel 
                firstBranch: {
                    echo 'firstBranch start';
                    sleep 5
                    echo 'firstBranch end'
                }, secondBranch: {
                    echo 'secondBranch start';
                    sleep 8
                    echo 'secondBranch end'
                }
        }
    }
}
